<% if (site.data.links){ %>
<div class="widget-wrap">
  <h3 class="widget-title">我的朋友们</h3>
  <div class="widget">
    <ul class="entry">
      <% for (var i in site.data.links){ %>
         <li class='link'><a href='<%- site.data.links[i] %>'><%= i %></a></li>
      <% } %>
    </ul>
  </div>
</div>
<% } %>